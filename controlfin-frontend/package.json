{
  "name": "controlfin-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,md}\"",
    "test": "vitest",
    "test:all": "vitest run --reporter=verbose --bail=0",
    "test:coverage": "vitest run --coverage",
    "validate:all": "npm run type-check && npm run lint && npm run test:all",
    "analyze": "vite-bundle-visualizer",
    "clean": "rm -rf dist node_modules",
    "validate:i18n": "node scripts/i18n-only-check.js",
    "validate:manual": "node scripts/manual-i18n-test.js",
    "validate:pre-merge": "node scripts/pre-merge-check.js",
    "validate:pre-pr": "node scripts/pre-pr-validation.js",
    "validate:hardcoded": "node scripts/hardcoded-strings-detector.js",
    "setup:hooks": "node scripts/setup-git-hooks.js",
    "setup:hooks:remove": "node scripts/setup-git-hooks.js remove"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@types/loglevel": "^1.5.4",
    "@types/papaparse": "^5.3.16",
    "@types/react-i18next": "^8.1.0",
    "@types/xlsx": "^0.0.35",
    "antd": "^5.27.4",
    "dayjs": "^1.11.18",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.3",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "loglevel": "^1.9.2",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^7.9.3",
    "sass": "^1.93.2",
    "sheetjs-style": "^0.15.8",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
